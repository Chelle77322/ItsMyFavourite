    "@babel/preset-env": "^7.18.6",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "@reduxjs/toolkit": "^1.8.3",
    "@types/bytes": "^3.1.1",
    "@types/ee-first": "^1.1.0",
    "@types/fbjs": "^3.0.2",
    "@types/http-errors": "^1.8.2",
    "@types/isomorphic-fetch": "^0.0.36",
    "async_hooks": "^1.0.0",
