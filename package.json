{
  "name": "itsmyfavourite",
  "version": "1.0.0",
  "description": "A REACT app that uses REDUX store and GooglePlaces API for users to search for all their favourite places and save them in one handy little app",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev && NODE_OPTIONS = '--experimental-specifier-resolution=node' node favourite/src/index.js",
    "transpile": "babel ./server --out-dir dist-server",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'favourite/*'\" \"npm run favourite\"",
    "favourite": "cd favourite && npm run start",
    "seed": "node seeders/seed.js",
    "install": "cd favourite && npm install",
    "build": "cd favourite && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "connected-react-router": "^6.9.2",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "npx": "^3.0.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.1.0",
    "react-google-maps": "^9.4.5",
    "react-google-maps-loader": "^4.3.0",
    "react-google-places-suggest": "^3.12.3",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "router": "^1.3.7",
    "sass": "^1.51.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chelle77322/ItsMyFavourite.git"
  },
  "keywords": [
    "GooglePlaces",
    "Favourites"
  ],
  "author": "Michelle Hall",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chelle77322/ItsMyFavourite/issues"
  },
  "devDependencies": {
    "@redux-devtools/core": "^3.12.0",
    "@reduxjs/toolkit": "^1.8.1",
    "eslint-plugin-react-hooks": "^4.5.0"
    
  }
}
