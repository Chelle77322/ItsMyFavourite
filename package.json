{
  "name": "itsmyfavourite",
  "version": "1.0.2",
  "type": "module",
  "description": "A REACT app that uses REDUX store and GooglePlaces API for users to search for all their favourite places and save them in one handy little app",
  "author": "Michelle Hall",
  "license": "ISC",
  "keywords": [
    "GooglePlaces",
    "Favourites"
  ],
  "homepage": "https://itsmyfavourite.herokuapp.com",
  "main": "server.js",
  "scripts": {
    "clear": "rimraf \"npm run favourite",
    "start": "npm run clear && node server.js",
    "start:prod": "node ./index.js",
    "start:dev": "concurrently \"nodemon --ignore 'favourite/*'\" \"npm run favourite\"",
    "favourite": "cd favourite && npm run start",
    "seed": "node seeders/seed.js",
    "install": "cd favourite && npm install",
    "build": "cd favourite && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "@types/fbjs": "^3.0.2",
    "async-hooks": "^1.3.1",
    "axios": "^0.27.2",
    "core-js": "^3.23.2",
    "express": "^4.18.1",
    "express-ws": "^5.0.2",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "if-env": "^1.0.4",
    "mongoose": "^6.4.0",
    "net": "^1.0.2",
    "net-browserify": "^0.1.4",
    "path-browserify": "^1.0.1",
    "plugin": "^0.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dom-server": "^0.0.5",
    "react-event-listener": "^0.6.6",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^6.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "rimraf": "^3.0.2",
    "rimraff": "^0.0.1-security",
    "sass": "^1.53.0"
  },
  "engines": {
    "npm": "8.12.2",
    "node": "18.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@types/react-router-config": "^5.0.6",
    "eslint-webpack-plugin": "^3.2.0",
    "webpack-node-externals": "^3.0.0"
  }
}
