{
  "name": "itsmyfavourite",
  "version": "1.0.2",
  "type": "module",
  "description": "A REACT app that uses REDUX store and GooglePlaces API for users to search for all their favourite places and save them in one handy little app",
  "author": "Michelle Hall",
  "license": "ISC",
  "keywords": [
    "GooglePlaces",
    "Favourites"
  ],
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'favourite/*'\" \"npm run favourite\"",
    "favourite": "cd favourite && npm run start",
    "seed": "node seeders/seed.js",
    "install": "cd favourite && npm install",
    "build": "cd favourite && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "async-hooks": "^1.3.1",
    "axios": "^0.27.2",
    "core-js": "^3.23.1",
    "express": "^4.18.1",
    "express-ws": "^5.0.2",
    "gh-pages": "^4.0.0",
    "if-env": "^1.0.4",
    "mongoose": "^6.4.0",
    "net": "^1.0.2",
    "net-browserify": "^0.2.4",
    "path-browserify": "^1.0.1",
    "plugin": "^0.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dom-server": "^0.0.5",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^6.3.0",
    "sass": "^1.52.3",
    "webpack": "^5.73.0"
  },
  "engines": {
    "npm": "8.12.2",
    "node": "18.3.0"
  },
  "devDependencies": {
    "@types/react-router-config": "^5.0.6",
    "webpack-node-externals": "^3.0.0"
  }
}
