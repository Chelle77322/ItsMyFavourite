{
  "name": "itsmyfavourite",
  "version": "1.0.2",
  "type": "module",
  "description": "A REACT app that uses REDUX store and GooglePlaces API for users to search for all their favourite places and save them in one handy little app",
  "author": "Michelle Hall",
  "license": "ISC",
  "keywords": [
    "GooglePlaces",
    "Favourites"
  ],
  "homepage": "http://chelle77322.github.io/ItsMyFavourite/",
  "scripts": {
    "build": "browserify ./src/client/client.js -o ./build/bundle.js -t babelify && babel ./src/ --out-dir ./build/",
    "watch": "concurrently \"watchify ./src/client/client.js -o ./build/bundle.js -t babelify -v\" \"babel ./src/ --out-dir ./build/ --watch\"",
    "start": "nodemon ./build/server/server.js"
  },
